@model IEnumerable<Cargo_Shipment.Models.UpdatNews>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_EmployeLayout.cshtml";
}

<div>
    <h4>Message for Employee</h4>
    <hr />
    @foreach (var item in Model)
    {
        <marquee>
            @Html.DisplayFor(modelItem => item.News)
            @Html.DisplayFor(modelItem => item.NewsDate)
        </marquee>

    }
</div>



